[Process]
If,ExistFile,%ProjectTemp%\Components_PluginCache_Extract.ini,Echo,"Components PluginCache"
If,Not,ExistFile,%ProjectTemp%\Components_PluginCache_Extract.ini,Run,%ScriptFile%,Process_Normal

[Process_Normal]
Call,HostOSArch
//--
StrFormat,Replace,%ScrollBox_RunFromWhere%," ","",%runfrom%
StrFormat,Replace,%CopyMode_ScrollBox%," ","",%CopyMode%
//--
If,ExistVar,%AtRam%,If,%runfrom%%AtRam%,Equal,RunDefaultFalse,Run,%ScriptFile%,SetRunfromCD
If,Not,ExistVar,%AtRam%,If,%runfrom%,Equal,RunDefault,Run,%ScriptFile%,SetRunfromCD
If,%runfrom%,Equal,RunfromCD,Run,%ScriptFile%,SetRunfromCD
//--
If,ExistDir,%AssemblyFolder%,DirDelete,%AssemblyFolder%
//--
//SP ermitteln
IniRead,%ProjectDir%\script.project,IniVariables,OSServicePack,%t%
If,Not,%t%,Equal,"",Set,%OSServicePack%,%t%
//--
If,ExistFile,%ProjectTemp%\HostOSTools\regedit.exe,Set,%RegeditexeFolder%,%ProjectTemp%\HostOSTools\
If,Not,ExistFile,%ProjectTemp%\HostOSTools\regedit.exe,If,Not,%HostOSArch%,Equal,x86,Echo,"regedit.exe MAY write wrong values",Warn
//--
If,%CopyMode%,Equal,full,Begin
Set,%FXVer%,3.0
Set,%FXVer1%,3
Set,%FXVer2%,0
Run,%ScriptFile%,CreateDirs,%AssemblyFolder%
Run,%ScriptFile%,fullCopy
End
If,%CopyMode%,Equal,fullwithoutReferenceAssemblies,Begin
Set,%FXVer%,3.0
Set,%FXVer1%,3
Set,%FXVer2%,0
Run,%ScriptFile%,CreateDirs,%AssemblyFolder%
Run,%ScriptFile%,fullCopyWithout
End
If,%CopyMode%,Equal,DotNet2.0only,Begin
Set,%FXVer%,2.0
Set,%FXVer1%,2
Set,%FXVer2%,0
Run,%ScriptFile%,CreateDirs,%AssemblyTemp%
Run,%ScriptFile%,CopyNet2.0Only
Run,%ScriptFile%,CopyAssemblyOnly
If,%SourceArch%,Equal,x64,Run,%ScriptFile%,CopyNet2.0Only,64
If,%SourceArch%,Equal,x64,Run,%ScriptFile%,CopyAssembly64Only
DirMove,%AssemblyTemp%,%AssemblyTarget%
End
If,%CopyMode%,Equal,DotNet2.0minimal,Begin
Set,%FXVer%,2.0
Set,%FXVer1%,2
Set,%FXVer2%,0
Run,%ScriptFile%,CreateDirs,%AssemblyTemp%
Run,%ScriptFile%,CopyNet2.0Core
Run,%ScriptFile%,CopyAssemblyCore
If,%SourceArch%,Equal,x64,Run,%ScriptFile%,CopyNet2.0Core,64
If,%SourceArch%,Equal,x64,Run,%ScriptFile%,CopyAssembly64Core
DirMove,%AssemblyTemp%,%AssemblyTarget%
End
//--
Run,%ScriptFile%,CopySystem
Run,%ScriptFile%,WriteSettings
//--
//Lancelot to get default view on explorer shell attrib ;)
Shellexecute,Hide,attrib.exe,"+S #$q%AssemblyFolder%#$q"

[SetRunfromCD]
Set,%AssemblyFolder%,%Target_Prog%\assembly
Set,%AssemblyTarget%,%Target_Prog%
Set,%DotNETFolder%,%Target_Prog%\Microsoft.NET
Set,%DotNetRunFrom%,CD

[CreateDirs]
If,Not,ExistDir,%DotNETFolder%\authman,DirMake,%DotNETFolder%\authman
If,Not,ExistDir,%DotNETFolder%\Framework,DirMake,%DotNETFolder%\Framework
If,Not,ExistDir,#1,DirMake,#1
If,Not,ExistDir,#1\GAC,DirMake,#1\GAC
If,Not,ExistDir,#1\GAC_32,DirMake,#1\GAC_32
If,Not,ExistDir,#1\GAC_MSIL,DirMake,#1\GAC_MSIL
If,Not,ExistDir,#1\temp,DirMake,#1\temp
If,Not,ExistDir,#1\tmp,DirMake,#1\tmp
If,%SourceArch%,Equal,x64,Begin
If,Not,ExistDir,%DotNETFolder%\Framework64,DirMake,%DotNETFolder%\Framework64
If,Not,ExistDir,#1\GAC_64,DirMake,#1\GAC_64
End
//--
DirMake,%AssemblyTemp%

[fullCopy]
Run,%ScriptFile%,fullCopyWithout
//--
//If,%OS%,Equal,Win7,Beg-in
If,Not,ExistDir,"%TargetDir%\Program Files\Reference Assemblies",DirMake,"%TargetDir%\Program Files\Reference Assemblies"
FileCopy,"%InstallSRC%\Program Files\Reference Assemblies\*.*","%TargetDir%\Program Files\Reference Assemblies"
If,%SourceArch%,Equal,x64,Begin
If,Not,ExistDir,"%TargetDir%\Program Files (x86)\Reference Assemblies",DirMake,"%TargetDir%\Program Files (x86)\Reference Assemblies"
FileCopy,"%InstallSRC%\Program Files (x86)\Reference Assemblies\*.*","%TargetDir%\Program Files (x86)\Reference Assemblies"
End
//En-d

[fullCopyWithout]
FileCopy,%InstallSRC%\Windows\Microsoft.NET\Framework\*.*,%DotNETFolder%\Framework
FileCopy,%InstallSRC%\Windows\assembly\*,%AssemblyFolder%\,NoRec
If,ExistDir,%InstallSRC%\Windows\assembly\GAC,FileCopy,%InstallSRC%\Windows\assembly\GAC\*.*,%AssemblyFolder%\GAC
If,ExistDir,%InstallSRC%\Windows\assembly\GAC_32,FileCopy,%InstallSRC%\Windows\assembly\GAC_32\*.*,%AssemblyFolder%\GAC_32
If,ExistDir,%InstallSRC%\Windows\assembly\GAC_MSIL,FileCopy,%InstallSRC%\Windows\assembly\GAC_MSIL\*.*,%AssemblyFolder%\GAC_MSIL
//--
If,%SourceArch%,Equal,x64,Begin
FileCopy,%InstallSRC%\Windows\Microsoft.NET\Framework64\*.*,%DotNETFolder%\Framework64
FileCopy,%InstallSRC%\Windows\assembly\GAC_64\*.*,%AssemblyFolder%\GAC_64
End

